struct propriete {
	std::string nom;
	int type;
	int taille;
	bool clé_primaire;
	bool auto_inc;
	std::any defaut;
	std::string table;
	std::string ref;
	int suppression;
	int ajournement;
};

ss << ' '
ss << "INT";
ss << "(" << taille << ")"
ss << ' '
ss << "NOT NULL"

