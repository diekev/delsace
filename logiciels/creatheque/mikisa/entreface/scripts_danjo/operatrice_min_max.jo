disposition "operatrice_min_max" {
    ligne {
	    étiquette(valeur="Nom Calque")
		liste(valeur="image"; attache=nom_calque; infobulle="Nom du calque à modifier.")
	}
	ligne {
	    étiquette(valeur="Neuf Min")
		couleur(valeur="0,0,0,1"; attache=neuf_min; min=0; max=1)
	}
	ligne {
	    étiquette(valeur="Neuf Max")
		couleur(valeur="1,1,1,1"; attache=neuf_max; min=0; max=1)
	}
}
