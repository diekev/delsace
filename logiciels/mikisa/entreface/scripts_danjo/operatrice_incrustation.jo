disposition "operatrice_incrustation" {
    ligne {
	    étiquette(valeur="Nom Calque")
		liste(valeur="image"; attache=nom_calque; infobulle="Nom du calque à modifier.")
	}
    ligne {
	    étiquette(valeur="Couleur")
		couleur(valeur="0,1,0,1"; attache=masque)
	}
	ligne {
	    étiquette(valeur="Angle")
		décimal(valeur=0.0; attache=angle)
	}
	ligne {
	    étiquette(valeur="A")
		décimal(valeur=1.0; attache=a)
	}
	ligne {
	    étiquette(valeur="B")
		décimal(valeur=1.0; attache=b)
	}
}
