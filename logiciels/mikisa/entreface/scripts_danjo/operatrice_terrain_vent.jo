disposition "operatrice_terrain_vent" {
	ligne {
		étiquette(valeur="Direction")
		décimal(valeur=0.0; attache=direction; animable)
	}
	ligne {
		étiquette(valeur="Force Descendante")
		décimal(valeur=0.5; attache=force1; animable)
	}
	ligne {
