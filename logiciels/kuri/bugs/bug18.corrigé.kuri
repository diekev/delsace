importe Support

Paginatrice :: struct {
    pages : [100][..]octet
}

principale :: fonc () -> z32
{
    paginatrice : Paginatrice

    page := paginatrice.pages[0]

    page = loge_tableau(octet, 4096)

    paginatrice.pages[0] = page

    retourne 0
}
