// erreur de compilation quand nous utilisons un type qui a le même nom qu'une fonction
// si la fonction est déclarée avant le type

importe Support

principale :: fonc () -> z32
{
    erreur_survenue := #exécute compile_script_erroné(«
        stat :: fonc ()
        {

        }

        stat :: struct {
            i: z32
        }

        principale :: fonc () -> z32
        {
            s : stat
            retourne 0
        }
    »)
    saufsi (erreur_survenue) {
        retourne 1
    }
    retourne 0
}
