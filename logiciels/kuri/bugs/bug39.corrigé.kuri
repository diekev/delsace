/* L'impression de la valeur des énumérations ne fonctionnent pas dans les
 * métaprogrammes
 * Catégorie : [exécution code, info type, type énum]
 */

importe Fondation

MonÉnum :: énum z32 {
    VALEUR
}

test_impression_énum :: fonc () -> bool
{
    a := MonÉnum.VALEUR
    c := imprime_chaine("%", a)
    diffère déloge(c)

    retourne c == "VALEUR"
}

#assert test_impression_énum()

principale :: fonc () -> z32
{
    saufsi test_impression_énum() {
        retourne 1
    }
    retourne 0
}
