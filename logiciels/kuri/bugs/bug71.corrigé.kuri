/* Compilation infinie et erreur de compilation quand la fonction principale est
 * également définit dans un autre espace de travail.
 * Catégorie : [compilation infini]
 */

importe Support

principale :: fonc () -> z32
{
    erreur_survenue := #exécute compile_script_erroné("
    principale :: fonc () -> z32
    {
        retourne 0
    }
    ")
    si erreur_survenue {
        retourne 1
    }
    retourne 0
}
