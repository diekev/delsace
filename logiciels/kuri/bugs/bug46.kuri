// boucle infinie quand il manque le ':' avant un type

importe Support

principale :: fonc () -> z32
{
    erreur_survenue := #exécute compile_script_erroné(«
    fonction :: fonc (a *rien) // interprété comme a * rien, mais le symbole 'a' n'est pas défini...
    {
    }
    »)
    saufsi (erreur_survenue) {
        retourne 1
    }
    retourne 0
}
