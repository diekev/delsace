// crash quand une discrimination possède une expression autre qu'une référence de déclaration

importe Support

TypeÉnum :: énum {
    X
    Y
    Z
}

principale :: fonc () -> z32
{
    erreur_survenue := #exécute compile_script_erroné("
    x : TypeÉnum

    discr x {
        5 comme TypeÉnum {

        }
    }
    ")

    saufsi (erreur_survenue) {
        retourne 1
    }
    retourne 0
}
