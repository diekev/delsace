// la capacité des tabeleaux n'est pas remise à zéro quand nous les délogeons

importe Fondation

principale :: fonc () -> z32
{
    tabl : []z32

    tableau_ajoute(@tabl, 1)
    tableau_ajoute(@tabl, 1)
    tableau_ajoute(@tabl, 1)
    tableau_ajoute(@tabl, 1)

    déloge tabl

    tableau_ajoute(@tabl, 1) // crash
    tableau_ajoute(@tabl, 1)

    déloge tabl

    retourne 0
}
