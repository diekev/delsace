/* Les paramètres des fonctions ne sont pas reconnus via type_de
 * Catégorie : [validation sémantique, paramètre fonction, expression type_de]
 */

importe Fondation

fonction :: fonc (a: z32, b: type_de(a))
{
    imprime("a : %, b : %\n", a, b)
}

principale :: fonc () -> z32
{
    fonction(5, 6)
    retourne 0
}
