// les infos_types des z64 ne sont pas générés correctement dans les métaprogrammes, la taille_en_octet est de 0...

importe Fondation

imprime_z64 :: fonc () -> bool
{
    x32 : z32 = 123456
    imprime("x32 = %\n", x32)

    x64 : z64 = 123456
    imprime("x64 = %\n", x64)

    info := info_de(z64)
    retourne info.taille_en_octet == 8
}

principale :: fonc () -> z32
{
    succès := #exécute imprime_z64()
    saufsi succès {
        retourne 1
    }
    retourne 0
}
