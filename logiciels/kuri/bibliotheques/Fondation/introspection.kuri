# Bibliothèque d'introspection.

charge "chaine"

fonc possède_membre(arg : eini, nom : chaine) -> bool
{
	info := arg.info

    si info.id != id_info.STRUCTURE {
		retourne faux
	}

	info_arg := transtype(info : *InfoTypeStructure)

    pour membre dans info_arg.membres {
        si membre.nom == nom {
			retourne vrai
        }
    }

	retourne faux
}

corout membres(arg : eini) -> chaine
{
	info := arg.info

    si info.id != id_info.STRUCTURE {
		retourne
	}

	info_arg := transtype(info : *InfoTypeStructure)

    pour membre dans info_arg.membres {
		retiens membre.nom
	}
}
