/* Triage des blocs « importe » des fichiers .kuri */

importe Fondation
importe OutilsMaintenance
importe SysFichier

principale :: fonc () -> z32
{
    args := arguments_ligne_commande()

    si args.taille < 2 {
        imprime("Utilisation : % FICHIER\n", args[0])
        retourne 1
    }

    chm := CheminFichier(args[1])
    exécute_rappel(chm, tri_imports_pour_fichier)
    retourne 0
}
