/* Triage des blocs « importe » des fichiers .kuri */

importe Fondation
importe LigneDeCommande
importe OutilsMaintenance
importe SysFichier

principale :: fonc ()
{
    chemin := parse_argument_chemin()
    saufsi chemin {
        exit(1)
    }

    extensions := [ ".kuri" ]
    chm := CheminFichier(chemin)
    exécute_rappel(chm, extensions, tri_imports_pour_fichier)
}
