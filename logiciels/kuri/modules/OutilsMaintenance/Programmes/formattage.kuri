importe Fondation
importe OutilsMaintenance
importe SysFichier

principale :: fonc () -> z32
{
    args := arguments_ligne_commande()

    si args.taille < 2 {
        imprime("Utilisation : % FICHIER\n", args[0])
        retourne 1
    }

    extensions := [".kuri"]
    chm := CheminFichier(args[1])
    exécute_rappel(chm, extensions, tri_imports_pour_fichier)
    exécute_rappel(chm, extensions, supprime_espaces_blancs_finaux)
    exécute_rappel(chm, extensions, supprime_lignes_vides_redondantes_pour_fichier)
    retourne 0
}
