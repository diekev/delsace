Entête :: struct {
    nom: chaine
    valeur: chaine
}

détruit_entête :: fonc (entête: &Entête)
{

}

cherche_entête :: fonc (entêtes: [..]Entête, clé: chaine) -> *Entête
{
    pour * entête dans entêtes {
        si entête.nom == clé {
            retourne entête
        }
    }

    retourne nul
}
