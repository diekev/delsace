	buf_length : n64
	// Set if we are doing several queries in one command ( as in LOAD TABLE ... FROM MASTER ), and do not want to confuse the client with OK at the wrong time
	where_b : n64
	max_packet : n64
	max_packet_size : n64
	pkt_nr : n32
	compress_pkt_nr : n32
	write_timeout : n32
	read_timeout : n32
