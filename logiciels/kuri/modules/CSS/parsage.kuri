#portée_export

donne_info_type_pour_nom :: fonc (nom: CSSOMString) -> *InfoTypePropriété
{
    pour infos_types_propriétés_shorthand {
        si it.nom == nom {
            retourne *infos_types_propriétés_shorthand[index_it]
        }
    }
    pour infos_types_propriétés {
        si it.nom == nom {
            retourne *infos_types_propriétés[index_it]
        }
    }
    retourne nul
}

