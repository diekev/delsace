/* https://www.w3.org/TR/css-cascade/#defaulting-keywords */
parse_css_wide_keyword :: fonc (parseuse: &Parseuse(ComponentValue)) -> Optionnel(ValeurDeStyle)
{
    mots_clés := [PROP_initial, PROP_inherit, PROP_unset, PROP_revert]
    retourne parse_mot_clé(parseuse, mots_clés)
}

parse_mot_clé :: fonc (parseuse: &Parseuse(ComponentValue), mots_clés: []CSSOMString) -> Optionnel(ValeurDeStyle)
{
    lexème := parseuse.lexème_courant()

    pour mots_clés {
        si lexème.est_identifiant(it) {
            parseuse.avance()
            retourne ValeurDeStyle(mot_clé = lexème.donne_texte())
        }
    }
}

/* https://drafts.csswg.org/css-values-4/#lengths */
parse_valeur_length :: fonc (parseuse: &Parseuse(ComponentValue)) -> Optionnel(ValeurDeStyle)
{
    si parseuse.fini() {
        retourne
    }

    lexème := parseuse.lexème_courant()

    si lexème.est_lexème_de_genre(GenreLexème.Dimension) {
        données_lexème := lexème.lexème

        si données_lexème.unité == PROP_em {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.Em)
            retourne ValeurDeStyle(longueur = longueur)
        }

        si données_lexème.unité == PROP_px {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.Px)
            retourne ValeurDeStyle(longueur = longueur)
        }

        si données_lexème.unité == PROP_in {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.In)
            retourne ValeurDeStyle(longueur = longueur)
        }

        si données_lexème.unité == PROP_cm {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.Cm)
            retourne ValeurDeStyle(longueur = longueur)
        }

        si données_lexème.unité == PROP_mm {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.Mm)
            retourne ValeurDeStyle(longueur = longueur)
        }

        si données_lexème.unité == PROP_Q {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.Q)
            retourne ValeurDeStyle(longueur = longueur)
        }

        si données_lexème.unité == PROP_pc {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.Pc)
            retourne ValeurDeStyle(longueur = longueur)
        }

        si données_lexème.unité == PROP_pt {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.Pt)
            retourne ValeurDeStyle(longueur = longueur)
        }

        si données_lexème.unité == PROP_rem {
            parseuse.avance()
            longueur := Longueur(données_lexème.numeric_value, Longueur.Unité.Rem)
            retourne ValeurDeStyle(longueur = longueur)
        }

        retourne
    }

    si lexème.est_lexème_de_genre(GenreLexème.Number) {
        parseuse.avance()

        si lexème.lexème.drapeau_type_nombre == DrapeauxTypeNombre.Integer {
            retourne ValeurDeStyle(integer = lexème.lexème.numeric_value)
        }

        retourne ValeurDeStyle(number = lexème.lexème.numeric_value)
    }
}

parse_valeur_percentage :: fonc (parseuse: &Parseuse(ComponentValue)) -> Optionnel(ValeurDeStyle)
{
    si parseuse.fini() {
        retourne
    }

    lexème := parseuse.lexème_courant()

    si lexème.est_lexème_de_genre(GenreLexème.Percentage) {
        parseuse.avance()
        retourne ValeurDeStyle(percentage = lexème.lexème.numeric_value)
    }

    si lexème.est_lexème_de_genre(GenreLexème.Number) {
        parseuse.avance()

        si lexème.lexème.drapeau_type_nombre == DrapeauxTypeNombre.Integer {
            retourne ValeurDeStyle(integer = lexème.lexème.numeric_value)
        }

        retourne ValeurDeStyle(number = lexème.lexème.numeric_value)
    }
}

parse_valeur_length_percentage :: fonc (parseuse: &Parseuse(ComponentValue)) -> Optionnel(ValeurDeStyle)
{
    length := parse_valeur_length(parseuse)
    si length.possède_valeur() {
        retourne length
    }
    retourne parse_valeur_percentage(parseuse)
}

parse_valeur_length_percentage_ou_auto :: fonc (parseuse: &Parseuse(ComponentValue)) -> Optionnel(ValeurDeStyle)
{
    lexème := parseuse.lexème_courant()
    si lexème.est_mot_clé(PROP_auto) {
        retourne ValeurDeStyle(mot_clé = PROP_auto)
    }
    retourne parse_valeur_length_percentage(parseuse)
}

