                sinon si lexeuse.code_référence_caractère > 0x10FFFF {
                    lexeuse.code_référence_caractère = CARACTÈRE_REMPLACEMENT
                }
                sinon si 0xD800 <= lexeuse.code_référence_caractère <= 0xDFFF {
