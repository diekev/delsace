                }
                sinon {
                    lexeuse.reconsomme_dans_état(lexeuse.état_retour)

                    si flush_points_de_code_référence_caractère(lexeuse) {
                        retourne défile(lexeuse.file_lexèmes)
                    }
                }
            }
            HexadecimalCharacterReference {
                si est_chiffre_ascii(c) {
