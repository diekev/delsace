importe Chaine
importe Ensemble
importe Fondation
importe Structures
importe Unicode

est_espace_blanc_html :: fonc (c: z8) -> bool #enligne
{
    retourne c == ' ' || c == '\t' || c == 0x0C || c == '\n'
}

est_espace_blanc_html :: fonc (c: n32) -> bool #enligne
{
    retourne c == 0x20 || c == 0x09 || c == 0x0C || c == 0x0A
}

GenreLexème :: énum {
    Doctype
    DébutTag
    FinTag
    Commentaire
    Caractère
    FinDeFichier
}

Lexème :: struct {
    genre: GenreLexème

    position: z32

    // général
    nom : DOMString = TAG_vide

    // doctype
    identifiant_public_doctype: ChaineUTF16
    identifiant_système_doctype: ChaineUTF16
    doctype_name_absent := vrai
    doctype_public_id_absent := vrai
    doctype_system_id_absent := vrai
    drapeau_force_quirks := faux

    // tag début ou fin
    auto_fermant := faux
    drapeau_auto_fermant_reconnu := faux
    attributs: [..]Attribut

    // caractère
    point_de_code: n32

    // commentaire
    données: ChaineUTF16
}

cherche_attribut :: fonc (lexème: *Lexème, nom: ChaineUTF16) -> *Attribut
{
    retourne cherche_attribut(lexème.attributs, crée_chaine_utf16_unique(nom))
}

cherche_attribut :: fonc (lexème: *Lexème, nom: DOMString) -> *Attribut
{
    retourne cherche_attribut(lexème.attributs, nom)
}

reconnait_drapeau_auto_fermant :: fonc (lexème: *Lexème)
{
    lexème.drapeau_auto_fermant_reconnu = lexème.auto_fermant
}

copie_lexème :: fonc (lexème: *Lexème) -> Lexème
{
    nouveau_lexème: Lexème
    nouveau_lexème.genre = lexème.genre
    nouveau_lexème.nom = lexème.nom

    pour lexème.attributs {
        ajoute_attribut(*nouveau_lexème.attributs, it.nom, it.valeur)
    }

    retourne nouveau_lexème
}

détruit_lexème :: fonc (lexème: *Lexème)
{
    // Ne pas détruire le nom du lexème car c'est un pointeur partagé !

    discr lexème.genre {
        Doctype {
            détruit_chaine(lexème.identifiant_public_doctype)
            détruit_chaine(lexème.identifiant_système_doctype)
        }
        DébutTag {
            détruit_attributs(lexème.attributs)
        }
        FinTag {
            // RÀF
        }
        Commentaire {
            détruit_chaine(lexème.données)
        }
        Caractère {
            // RÀF
        }
        FinDeFichier {
            // RÀF
        }
    }
}

