                    enfile(lexeuse.file_lexèmes, lexeuse.émet_lexème_fin_de_fichier())
                    retourne défile(lexeuse.file_lexèmes)
                }
                sinon {
                    chaine_ajoute(*lexeuse.lexème_courant.données, '-')
                    lexeuse.reconsomme_dans_état(ÉtatLexage.Comment)
                }
