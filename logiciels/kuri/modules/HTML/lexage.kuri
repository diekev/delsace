                }
                sinon si c == FIN_DE_FICHIER {
                    retourne lexeuse.émet_lexème_fin_de_fichier()
                }
                sinon {
                    lexeuse.état = ÉtatLexage.ScriptDataEscaped
                    retourne lexeuse.émet_lexème_caractère(c)
                }
