                sinon si est_lettre_majuscule_ascii(c) {
                    chaine_ajoute(*lexeuse.nom_lexème_courant, c + 0x20)
                    chaine_ajoute(*lexeuse.tampon_temporaire, c)
                    continue
                }
                sinon si est_lettre_minuscule_ascii(c) {
                    chaine_ajoute(*lexeuse.nom_lexème_courant, c)
                    chaine_ajoute(*lexeuse.tampon_temporaire, c)
                    continue
                }

                // Garde la mémoire
                réinitialise(lexeuse.nom_lexème_courant)

                lexeuse.reconsomme_dans_état(ÉtatLexage.RAWTEXT)

                enfile(lexeuse.file_lexèmes, lexeuse.émet_lexème_caractère(CHEVRON_OUVRANT))
                enfile(lexeuse.file_lexèmes, lexeuse.émet_lexème_caractère(SLASH))
