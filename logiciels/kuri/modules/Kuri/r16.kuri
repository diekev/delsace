DLS_vers_r32 :: fonc (h: r16) -> r32 #interface #externe

DLS_vers_r64 :: fonc (a: r16) -> r64 #interface #externe

DLS_depuis_r32 :: fonc (v: r32) -> r16 #interface #externe

DLS_depuis_r64 :: fonc (v: r64) -> r16 #interface #externe

/* ************* opérateurs unaires ************* */

opérateur + :: fonc (a: r16) -> r16
{
	retourne a
}

opérateur - :: fonc (a: r16) -> r16
{
	f := DLS_vers_r32(a)
	retourne DLS_depuis_r32(-f)
}

/* ************* addition ************* */

opérateur + :: fonc (a: r16, b: r16) -> r16
{
	f1 := DLS_vers_r32(a)
	f2 := DLS_vers_r32(b)
	retourne DLS_depuis_r32(f1 + f2)
}

opérateur + :: fonc (a: r16, b: r32) -> r16
{
	f1 := DLS_vers_r32(a)
	retourne DLS_depuis_r32(f1 + b)
}

opérateur + :: fonc (a: r32, b: r16) -> r16
{
	f1 := DLS_vers_r32(b)
	retourne DLS_depuis_r32(a + f1)
}

opérateur + :: fonc (a: r16, b: r64) -> r16
{
	f1 := DLS_vers_r64(a)
	retourne DLS_depuis_r64(f1 + b)
}

opérateur + :: fonc (a: r64, b: r16) -> r16
