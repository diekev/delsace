}

/* ************* multiplication ************* */

opérateur * :: fonc (a: r16, b: r16) -> r16
{
	f1 := DLS_vers_r32(a)
	f2 := DLS_vers_r32(b)
	retourne DLS_depuis_r32(f1 * f2)
}

opérateur * :: fonc (a: r16, b: r32) -> r16
