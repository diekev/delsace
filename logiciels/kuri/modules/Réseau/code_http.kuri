    NOT_IMPLEMENTED                 := 501
    BAD_GATEWAY                     := 502
    SERVICE_UNAVAILABLE             := 503
    GATEWAY_TIMEOUT                 := 504
    HTTP_VERSION_NOT_SUPPORTED      := 505
    NETWORK_AUTHENTICATION_REQUIRED := 511
}

chaine_code_état_http :: fonc (code: CodeÉtatHTTP) -> chaine
{
    discr code {
        CONTINUE                        { retourne "100 CONTINUE"; }
        SWITCHING_PROTOCOL              { retourne "101 SWITCHING PROTOCOL"; }
        OK                              { retourne "200 OK"; }
        CREATED                         { retourne "201 CREATED"; }
