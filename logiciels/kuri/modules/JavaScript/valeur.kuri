to_object :: fonc (realm: *Realm, valeur: Valeur) -> *Object
{
    si valeur.est_undefined() {
        _ := lance_type_error(realm, "undefined is not an Object")
        retourne nul
    }

    si valeur.est_nulle() {
        _ := lance_type_error(realm, "null is not an Object")
        retourne nul
    }

    si valeur.est_object() {
        retourne valeur.donne_objet()
    }

    si valeur.est_chaine() {
        chn := valeur.String
        saufsi chn.objet {
            chn.objet = cr√©e_chaine(realm.donne_tas_de_m√©moire(), realm.intrinsics.string_prototype, valeur.String)
        }
        retourne chn.objet
    }

    si valeur.est_nombre() {
        retourne cr√©e_number_object(realm.donne_tas_de_m√©moire(), realm.intrinsics.number_prototype, valeur.Number)
    }

    si valeur.est_boolean() {
        retourne cr√©e_boolean_object(realm, valeur.Boolean)
    }

    panique("to_object non-impl√©ment√© pour type")
}

/* 7.1.19 ToPropertyKey ( argument )
 * https://tc39.es/ecma262/multipage/abstract-operations.html#sec-topropertykey */
