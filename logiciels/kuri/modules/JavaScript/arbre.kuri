    DécalageDroite /* >>= */
    DécalageDroiteNaturel /* >>>= */
    EtBinaire /* &= */
    OuBinaire /* |= */
    OuExBinaire /* ^= */
    Coalesce /* ??= */
    OuLogique /* ||= */
    EtLogique /* &&=*/
}

AssignmentExpression :: struct {
