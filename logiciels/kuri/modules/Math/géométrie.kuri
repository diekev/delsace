/** \nom Point3D
 * \{ */

Point3D :: struct {
    x: r32 @accessible
    y: r32 @accessible
    z: r32 @accessible
} @Python

opérateur - :: (a: Point3D, b: Point3D) -> Vec3
{
    résultat : Vec3 = ---
    résultat.x = a.x - b.x
    résultat.y = a.y - b.y
    résultat.z = a.z - b.z
    retourne résultat
}

