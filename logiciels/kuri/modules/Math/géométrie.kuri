/* ------------------------------------------------------------------------- */
/** \nom Point2D
 * \{ */

Point2D :: struct ($T: type_de_données) {
    x: T @mutable
    y: T @mutable
}

donne_point_2d_réel :: fonc (point: Point2D(z32)) -> Point2D(r32)
{
    retourne Point2D(r32)(point.x comme r32, point.y comme r32)
}

donne_boite_englobante :: fonc (points: ...Point2D($T)) -> RectanglePosDim(T)
{
    rectangle_pur : RectanglePur(T) = ---
    rectangle_pur.gauche = infinité_positive(T)
    rectangle_pur.droite = -infinité_positive(T)
    rectangle_pur.haut = -infinité_positive(T)
    rectangle_pur.bas = infinité_positive(T)

    pour points {
        si it.x < rectangle_pur.gauche {
            rectangle_pur.gauche = it.x
        }
        si it.x > rectangle_pur.droite {
            rectangle_pur.droite = it.x
        }
        si it.y < rectangle_pur.bas {
            rectangle_pur.bas = it.y
        }
        si it.y > rectangle_pur.haut {
            rectangle_pur.haut = it.y
        }
    }

    retourne donne_rectangle_pos_dim(rectangle_pur)
}

donne_distance_carrée :: fonc (p0: Point2D($T), p1: Point2D(T)) -> T
{
    dx := p1.x - p0.x
    dy := p1.y - p0.y
    retourne dx * dx + dy * dy
}

opérateur == :: (p0: Point2D($T), p1: Point2D(T)) -> bool
{
    retourne p0.x == p1.x && p0.y == p1.y
}

opérateur + :: (p0: Point2D($T), p1: Point2D(T)) -> Point2D(T)
{
    retourne Point2D(T)(p0.x + p1.x, p0.y + p1.y)
}

opérateur + :: (p0: Point2D(r32), v: Vec2) -> Point2D(r32)
{
    retourne Point2D(r32)(p0.x + v.x, p0.y + v.y)
}

opérateur - :: (p0: Point2D(r32), p1: Point2D(r32)) -> Vec2
{
    retourne Vec2(p0.x - p1.x, p0.y - p1.y)
}

opérateur * :: (p0: Point2D($T), r: T) -> Point2D(T)
{
    retourne Point2D(T)(p0.x * r, p0.y * r)
}

opérateur / :: (p0: Point2D($T), r: T) -> Point2D(T)
{
    retourne Point2D(T)(p0.x / r, p0.y / r)
}

/** \} */

