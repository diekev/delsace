importe "flux"

imprime_compteur :: fonc() -> rien
{
    imprime_sans_format("Compteur = ", ctx.compteur, '\n')
}

incrémente_compteur :: fonc() -> rien
{
    ctx.compteur += 1
}

principale :: fonc(args : []*z8) -> z32
{
    imprime_compteur()
    incrémente_compteur()
    imprime_compteur()
    retourne 0
}
