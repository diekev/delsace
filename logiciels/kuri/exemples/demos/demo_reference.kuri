importe "flux"

FOO :: struct {
    a := 123
}

bar :: fonc(f : &FOO) -> rien
{
    f.a = 987
}

foo :: fonc(a : &z32) -> rien
{
    a := 7
}

échange :: fonc(a : &z32, b : &z32) -> rien
{
    tmp := a
    a = b
    b = tmp
}

principale :: fonc () -> z32
{
    a := 5
    imprime_sans_format("a = ", a, '\n')

    foo(a)

    imprime_sans_format("a = ", a, '\n')

    //#####################

    foo : FOO

    imprime_sans_format(foo, '\n')

    bar(foo)

    imprime_sans_format(foo, '\n')

    //#####################

    x := 123
    y := 456
    imprime_sans_format("x = ", x, ", y = ", y, '\n')

    échange(x, y)
    imprime_sans_format("x = ", x, ", y = ", y, '\n')

    //#####################

    z := 5
    w : &z32 = z

    imprime_sans_format("z = ", z, '\n')

    w := 10

    imprime_sans_format("z = ", z, '\n')

    //#####################

    retourne 0
}
