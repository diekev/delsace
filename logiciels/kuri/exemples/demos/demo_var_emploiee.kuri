importe "flux"

TEST :: struct {
    x := 5
    y := "une chaine au hasard"
    z := 0.35
}

imprime_test :: fonc(empl test : TEST) -> rien
{
    imprime_sans_format("x = ", x, '\n')
    imprime_sans_format("y = ", y, '\n')
    imprime_sans_format("z = ", z, '\n')
}

modifie_test :: fonc(dyn empl test : *TEST) -> rien
{
    x = 7
    y = "chaine modifiÃ©e"
    z = 135.75
}

principale :: fonc(args : []*z8) -> z32
{
    test : TEST
    imprime_test(test)
    modifie_test(@test)
