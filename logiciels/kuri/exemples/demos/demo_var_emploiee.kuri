importe Fondation

TEST :: struct {
    x := 5
    y := "une chaine au hasard"
    z := 0.35
}

imprime_test :: fonc(empl test : TEST) -> rien
{
    imprime("x = %\n", x)
    imprime("y = %\n", y)
    imprime("z = %\n", z)
}

modifie_test :: fonc(empl test : *TEST) -> rien
{
    x = 7
    y = "chaine modifiÃ©e"
    z = 135.75
}

principale :: fonc () -> z32
{
    test : TEST
    imprime_test(test)
    modifie_test(*test)
    imprime_test(test)

    retourne 0
}
