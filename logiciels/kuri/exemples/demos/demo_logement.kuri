importe "flux"

principale :: fonc () -> z32
{
	diffère {
	    imprime_sans_format("Mémoire utilisée : ", mémoire_utilisée(), "\n")
	}

	x := loge chaine(256) sinon {
	    retourne 1
	}

	diffère { déloge x }

	y := loge r64 sinon {
	    retourne 1
	}

	diffère { déloge y }

	reloge x : chaine(512) sinon {
	    retourne 1
	}

    reloge y : r64 sinon {
	    retourne 1
	}

	imprime("Mémoire utilisée : ", mémoire_utilisée(), "\n")


    retourne 0
}
