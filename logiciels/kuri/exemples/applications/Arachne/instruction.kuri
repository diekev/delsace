importe Fondation

énum Instruction : z32 {
    CREE
    SUPPRIME
    TROUVE
    RETOURNE
    INVALIDE
}

fonc détermine_instruction(chn : chaine) : Instruction
{
    info = info_de(Instruction.CREE)

    pour nom, idx dans info.noms {
        si nom == chn {
            retourne transtype(info.valeurs[idx] : Instruction)
        }
    }

    retourne Instruction.INVALIDE
}
