importe Fondation

Drapeau :: énum_drapeau z32 {
    FA
    FB
    FC
    FD
    FE
}

Normal :: énum z32 {
    FA
    FB
    FC
    FD
    FE
}

principale :: fonc () -> z32
{
    drapeau : Drapeau

    imprime("drapeau = %\n", drapeau)

    drapeau |= Drapeau.FA

    imprime("drapeau = % (%)\n", drapeau, drapeau comme z32)

    drapeau |= Drapeau.FB

    imprime("drapeau = % (%)\n", drapeau, drapeau comme z32)

    drapeau |= Drapeau.FE
    drapeau |= Drapeau.FD

    imprime("drapeau = % (%)\n", drapeau, drapeau comme z32)

    normal : Normal

    imprime("normal = %\n", normal)

    normal = Normal.FA

    imprime("normal = %\n", normal)

    // À définir si ceci est possible ou non
    normal |= Normal.FB
    normal |= Normal.FC

    imprime("normal = %\n", normal)

    retourne 0
}
