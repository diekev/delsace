importe Fondation

Drapeau :: énum_drapeau z32 {
    FA
    FB
    FC
    FD
    FE
}

Normal :: énum z32 {
    FA
    FB
    FC
    FD
    FE
}

principale :: fonc () -> z32
{
    dyn drapeau : Drapeau

    imprime_sans_format("drapeau = ", drapeau, "\n")

    drapeau |= Drapeau.FA

    imprime_sans_format("drapeau = ", drapeau, " (", transtype(drapeau : z32), ")\n")

    drapeau |= Drapeau.FB

    imprime_sans_format("drapeau = ", drapeau, " (", transtype(drapeau : z32), ")\n")

    drapeau |= Drapeau.FE
    drapeau |= Drapeau.FD

    imprime_sans_format("drapeau = ", drapeau, " (", transtype(drapeau : z32), ")\n")

    dyn normal : Normal

    imprime_sans_format("normal = ", normal, "\n")

    normal = Normal.FA

    imprime_sans_format("normal = ", normal, "\n")

    // À définir si ceci est possible ou non
    normal |= Normal.FB
    normal |= Normal.FC

    imprime_sans_format("normal = ", normal, "\n")

    retourne 0
}
