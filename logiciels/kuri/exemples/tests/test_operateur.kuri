Vecteur3D :: struct {
    x: n32
    y: n32
    z: n32
}

racine_carré :: fonc (x: n32) -> n32
{
    num := x
    res : n32 = 0

    // Le second bit est mis en place
    // n8  : 1 << 6
    // n16 : 1 << 14
    // n32 : 1 << 30
    // n64 : 1 << 62
    bit : n32 = 1 << 30

    boucle {
        si bit <= num {
            arrête
        }

        bit = bit >> 2
    }

    boucle {
        si bit == 0 {
            arrête
        }

        si num >= (res + bit) {
            num = num - (res + bit)
            res = res + (bit << 1)
        }

        res = res >> 1
        bit = bit >> 2
    }

    retourne res
    /*
    si (x == 0) || (x == 1) {
        retourne x;
    }

    i := 1;
    res := 1;

    boucle {
        si res > x {
            arrête;
        }

        i = i + 1;
        res = i * i;
    }

    retourne i - 1;
*/
}

carré :: fonc (x: n32) -> n32
{
    retourne x * x
}

longueur_vecteur :: fonc (v: Vecteur3D) -> n32
{
    retourne 0
}

longueur :: fonc (x: n32, y: n32, z: n32) -> n32
{
    retourne racine_carré(carré(x) + carré(y) + carré(z))
}

