test_discr :: fonc (a: z32) -> bool
{
    discr a {
        1, 2, 3 {
            retourne vrai
        }
        8, 9, 10 {
            retourne vrai
        }
        sinon {
            retourne faux
        }
    }

    retourne faux
}

