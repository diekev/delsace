importe "flux"

plage :: corout(début : z32, fin : z32, pas : z32) -> z32
{
    taille := (fin - début) / pas
    dyn val := début

    pour i dans 0 ... taille {
        retiens val
        val += pas
    }

    imprime("Fin coroutine\n")
}

principale :: fonc () -> z32
{
    pour i dans plage(0, 10, 1) {
        imprime_sans_format("Valeur = ", i, '\n')
    }

    retourne 0
}
