importe "chaine"
importe "flux"

principale :: fonc () -> z32
{
    imprime_sans_format("Bonjour, tout le monde !\n")
    imprime_sans_format("Nous allons imprimer les arguments :\n")

    pour arg, index dans args {
        chn := converti_chaine_c(arg)
        imprime_sans_format("Argument ", index, " = ", chn, '\n')
    }

    retourne 0
}
